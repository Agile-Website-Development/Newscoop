{
    "name":"sourcefabric/newscoop",
    "description":"The open source CMS for news",
    "keywords":["cms,journalism,publication"],
    "homepage":"http://www.sourcefabric.org/en/newscoop/",
    "version":"4.0",
    "license":"GPL-3.0",
    "authors":[],
    "require":{
        "php": ">=5.3.0",
        "zendframework/zendframework1":"1.11.12",
        "Smarty3":"3.0.8",
        "doctrine/orm":"2.1.*",
        "sfDependencyInjection":"2.0",
        "sfEventDispatcher":"2.0",
        "fpn/doctrine-extensions-taggable":">=2.0",
        "gedmo/doctrine-extensions":"=v2.1.0",
        "symfony/console":"2.0.x-dev"
    },
    "repositories":[
        {
            "type":"package",
            "package":{
                "name":"sfDependencyInjection",
                "version":"2.0",
                "source":{
                    "url":"https://github.com/fabpot/dependency-injection.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"sfEventDispatcher",
                "version":"2.0",
                "source":{
                    "url":"https://github.com/fabpot/event-dispatcher.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"Smarty3",
                "version":"3.0.8",
                "dist":{
                    "url":"http://www.smarty.net/files/Smarty-3.0.8.zip",
                    "type":"zip"
                },
                "source":{
                    "url":"http://smarty-php.googlecode.com/svn/",
                    "type":"svn",
                    "reference":"tags/Smarty_3_0_8//distribution/"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"zendframework/zendframework1",
                "version":"1.11.12",
                "dist":{
                    "url":"http://framework.zend.com/releases/ZendFramework-1.11.12/ZendFramework-1.11.12.zip",
                    "type":"zip"
                },
                "source":{
                    "url":"http://framework.zend.com/svn/framework/standard/",
                    "type":"svn",
                    "reference":"tags/release-1.11.12/"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library/"
                    }
                },
                "include-path": [
                    "library/"
                ],
                "config": {
                    "bin-dir": "bin"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"gedmo/doctrine-extensions",
                "version":"2.1",
                "dist":{
                    "url":"https://github.com/l3pp4rd/DoctrineExtensions/zipball/v2.1.0",
                    "type":"zip"
                },
                "source":{
                    "url":"https://github.com/l3pp4rd/DoctrineExtensions.git",
                    "type":"git",
                    "reference":"tags/v2.1.0"
                }
            }
        }

    ],

    "autoload":{
        "psr-0": {
            "Newscoop": "newscoop/library",
            "Resource": "newscoop/library",
            "Nette": "newscoop/library",
            "Gedmo": "vendor/gedmo/doctrine-extensions/lib/",
            "Zend": "vendor/zendframework/zendframework1/library"
        },
        "classmap":["vendor/sfDependencyInjection", "vendor/sfEventDispatcher", "vendor/Smarty3/libs", "newscoop/classes","dependencies/include/adodb","newscoop/template_engine","newscoop/plugins"]
    },
    "minimum-stability":"dev"
}
